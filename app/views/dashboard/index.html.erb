<div class="col-md-2 col-md-offset-10">
<%= link_to 'Log Out', destroy_user_session_path, method: :delete, class: 'text-right'%>
</div>
<% if !current_user.access_token %>
  <%= render 'not_authenticated', client: @client %>

<% else %>
  <%= render 'authenticated', client: @client %>
  <%= render 'progress_bar' %>
<% end %>

